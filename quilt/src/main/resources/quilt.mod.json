{
  "schema_version": 1,
  "mixin": [
    "terrariamod.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "terrariamod",
    "version": "${version}",
    "metadata": {
      "name": "TerrariaMod",
      "description": "A recreation of the popular game Terraria for Minecraft!",
      "authors": ["Ryorama"],
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.ryorama.terrariamod.quilt.TerrariaModQuilt"
      ],
      "client": [
        "com.ryorama.terrariamod.quilt.TerrariaModQuiltClient"
      ],
      "modmenu": [
        "com.ryorama.terrariamod.fabric.ModMenuIntegration"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20"
      },
      {
        "id": "architectury",
        "version": ">=9.0.8"
      }
    ]
  }
}